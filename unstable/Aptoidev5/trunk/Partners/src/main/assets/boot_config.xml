<?xml version="1.0" encoding="utf-8"?>
<bootconf>
    <storeconf>
        <theme>default</theme>
        <avatar>https://www.aptoide.com/includes/themes/default/images/repo_default_icon.png</avatar>
        <description></description>
        <view>list</view>
        <items>applications,games,top_apps,latest_apps,latest_comments,latest_likes,favorites,recommended</items>
    </storeconf>
    <aptoideconf>
        <partnertype>premium</partnertype>
        <partnerid>[your partnerid]</partnerid>
        <marketname>Aptoide Appstore</marketname>
        <defaultstorename>apps</defaultstorename>
        <aptoidetheme>default</aptoidetheme>
        <splashscreen>[your portrait splashscreen url]</splashscreen>
        <splashscreenland>[your landscape splashscreen url]</splashscreenland>
        <brand>brand_aptoide</brand>
        <searchstores>true</searchstores>
        <maturecontentswitch>false</maturecontentswitch>
        <maturecontentswitchvalue>true</maturecontentswitchvalue>
        <multiplestores>false</multiplestores>
        <customeditorschoice>true</customeditorschoice>
        <adunitid>[your mobfox adunit id]</adunitid>
        <createshortcut>true</createshortcut>
    </aptoideconf>
</bootconf>
